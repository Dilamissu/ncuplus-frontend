    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@tailwindcss/forms": "^0.4.0",
    "@tweenjs/tween.js": "^18.6.4",
    "axios": "^0.25.0",
    "dotenv": "^14.2.0",
    "esbuild": "^0.14.11",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@vitejs/plugin-vue": "^2.0.0",
